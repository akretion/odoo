<odoo>
    <record model="ir.ui.view" id="hello_world.list">
        <field name="name">hello_world list</field>
        <field name="model">phs.hello.world</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="message" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="hello_world_action_window">
        <field name="name">hello_world window</field>
        <field name="res_model">phs.hello.world</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem name="Hello_world" id="menu_root" sequence="5" />
    <menuitem name="Hello" id="hello_world.hello" parent="menu_root" />
    <menuitem
        name="Say hello"
        id="hello_world.say_hello"
        parent="hello"
        action="hello_world_action_window"
    />
</odoo>
